services:
  cvrecognizingservice.api:
    build:
      context: D:\Web CV Hosting\src\Services\CVRecognizingService
      dockerfile: CVRecognizingService.API/Dockerfile
      args:
        BUILD_CONFIGURATION: Debug
      labels:
        com.microsoft.created-by: visual-studio
        com.microsoft.visual-studio.project-name: CVRecognizingService.API
      target: base
    container_name: cvrecognizingservice
    depends_on:
      mongodb:
        condition: service_started
        required: true
    entrypoint:
      - tail
      - -f
