      com.microsoft.visualstudio.debuggee.killprogram: /bin/sh -c "if PID=$$(pidof dotnet); then kill $$PID; fi"
      com.microsoft.visualstudio.debuggee.program: dotnet
      com.microsoft.visualstudio.debuggee.workingdirectory: /app
    links:
      - mongodb:mdb
    networks:
      cvrecognizing-web: null
    ports:
      - mode: ingress
        target: 8080
        published: "5000"
        protocol: tcp
      - mode: ingress
        target: 8081
        published: "5001"
        protocol: tcp
    tty: true
    volumes:
      - type: bind
        source: D:\Web CV Hosting\src\Services\CVRecognizingService\CVRecognizingService.API
        target: /app
